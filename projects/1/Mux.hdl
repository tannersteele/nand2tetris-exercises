/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    // Without the 'NOT' gate on the SEL pin of 'AND gate A', conditional branching wouldn't be possible. It denotes 0 being the state for A, and 1 for B.
    Not(in=sel,           out=notsel);
    And(a=a,    b=notsel, out=anda);
    And(a=b,    b=sel,    out=andb);
    Or (a=anda, b=andb,   out=out);
}